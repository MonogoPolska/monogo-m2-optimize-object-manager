{
    "name": "monogo/optimize-object-manager",
    "description": "Magento 2 module to optimize Compiled ObjectManager",
    "require": {
        "magento/framework": ">=102.0.4",
        "cweagans/composer-patches": "^1.6"
    },
    "type": "magento2-module",
    "version": "1.0.3",
    "license": "MIT",
    "keywords": ["Magento 2", "optimization", "ObjectManager"],
    "authors": [
        {
            "name": "Monogo Sp z o.o.",
            "email": "pawel.detka@monogo.pl"
        }
    ],
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {
            "magento/framework": {
                "Optimize Magento ObjectManager ConfigLoader: ": "https://raw.githubusercontent.com/MonogoPolska/monogo-m2-optimize-object-manager/master/patches/composer/optimize-config-loader.diff"
            }
        }
    }
}
